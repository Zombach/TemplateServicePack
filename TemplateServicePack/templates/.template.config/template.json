{
  "$schema": "http://json.schemastore.org/template",
  "author": "Stanislav Kozlov",
  "classifications": [ "Web", "Service" ],
  "identity": "Template.Service.CSharp",
  "name": "Template Service",
  "shortName": "srv",
  "sourceName": "ServiceName",
  "preferNameDirectory": true,
  "tags": {
    "language": "C#",
    "type": "solution"
  },
  "symbols": {
    "PodId": {
      "type": "parameter",
      "description": "PodId",
      "defaultValue": "service",
      "replaces": "{PRODID}"
    },
    "LogFilePath": {
      "type": "parameter",
      "description": "Log file path",
      "defaultValue": "../../logs/service-.log",
      "replaces": "{LOGFILEPATH}"
    },
    "Framework": {
      "type": "parameter",
      "description": "The target .NET CORE for the project.",
      "datatype": "choice",
      "choices": [
        {
          "choice": "net8.0",
          "description": "Target .NET 8.0"
        }
      ],
      "defaultValue": "net8.0",
      "replaces": "net8.0"
    }
  }
}
